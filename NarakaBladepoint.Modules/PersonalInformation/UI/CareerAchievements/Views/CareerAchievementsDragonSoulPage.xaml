            <controls:HighlightBorder Margin="5,10,5,0" EnableMaskOnHover="True">
                <Image Source="{StaticResource PersonalInfoDetailsAchievementsImages3_2Image}" />
            </controls:HighlightBorder>
            <controls:HighlightBorder Margin="5,10,5,0" EnableMaskOnHover="True">
                <Image Source="{StaticResource PersonalInfoDetailsAchievementsImages3_3Image}" />
            </controls:HighlightBorder>
            <controls:HighlightBorder Margin="5,10,5,0" EnableMaskOnHover="True">
                <Image Source="{StaticResource PersonalInfoDetailsAchievementsImages3_4Image}" />
            </controls:HighlightBorder>
            <controls:HighlightBorder Margin="5,10,5,0" EnableMaskOnHover="True">
                <Image Source="{StaticResource PersonalInfoDetailsAchievementsImages3_5Image}" />
