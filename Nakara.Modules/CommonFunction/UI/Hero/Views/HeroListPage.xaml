<base:UserControlBase
    x:Class="Nakara.Modules.CommonFunction.UI.Hero.Views.HeroListPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:attach="clr-namespace:Nakara.Framework.UI.AttachedProperties;assembly=Nakara.Framework"
    xmlns:base="clr-namespace:Nakara.Framework.Core.Bases;assembly=Nakara.Framework"
    xmlns:controls="clr-namespace:Nakara.Controls;assembly=Nakara.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewmodels="clr-namespace:Nakara.Modules.CommonFunction.UI.Hero.ViewModels"
    attach:EscKeyAttachedProperty.Command="{Binding ReturnCommand}"
    d:Background="Gray"
    d:DataContext="{d:DesignInstance Type=viewmodels:HeroListPageViewModel}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <controls:EscBackControl
            HorizontalAlignment="Right"
            VerticalAlignment="Bottom"
            Command="{Binding ReturnCommand}" />
    </Grid>
</base:UserControlBase>