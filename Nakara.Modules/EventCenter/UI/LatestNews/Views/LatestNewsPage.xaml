<bases:UserControlBase
    x:Class="Nakara.Modules.EventCenter.UI.LatestNews.Views.LatestNewsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:bases="clr-namespace:Nakara.Framework.Core.Bases;assembly=Nakara.Framework"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Nakara.Modules.EventCenter.UI.LatestNews.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="720"
    d:DesignWidth="1080"
    mc:Ignorable="d">
    <Grid>
        <Grid Margin="50,10">
            <Grid Margin="0,40,0,30">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Image
                    Grid.Column="0"
                    Margin="5,0"
                    Source="pack://application:,,,/Nakara.Resources;component/Image/Region/EventCenter/LatestNews/1.png"
                    Stretch="UniformToFill" />
                <Grid Grid.Column="1" Margin="5,0">
                    <UniformGrid Rows="2">
                        <Image
                            Margin="0,5"
                            Source="pack://application:,,,/Nakara.Resources;component/Image/Region/EventCenter/LatestNews/2.png"
                            Stretch="UniformToFill" />
                        <Image
                            Margin="0,5"
                            Source="pack://application:,,,/Nakara.Resources;component/Image/Region/EventCenter/LatestNews/4.png"
                            Stretch="UniformToFill" />
                    </UniformGrid>
                </Grid>
                <Grid Grid.Column="2" Margin="5,0">
                    <UniformGrid Rows="2">
                        <Image
                            Margin="0,5"
                            Source="pack://application:,,,/Nakara.Resources;component/Image/Region/EventCenter/LatestNews/3.png"
                            Stretch="UniformToFill" />
                        <Image
                            Margin="0,5"
                            Source="pack://application:,,,/Nakara.Resources;component/Image/Region/EventCenter/LatestNews/5.png"
                            Stretch="UniformToFill" />
                    </UniformGrid>
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</bases:UserControlBase>