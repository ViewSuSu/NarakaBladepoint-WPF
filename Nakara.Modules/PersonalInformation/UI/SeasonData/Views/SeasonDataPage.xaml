<bases:UserControlBase
    x:Class="Nakara.Modules.PersonalInformation.UI.SeasonData.Views.SeasonDataPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:bases="clr-namespace:Nakara.Framework.Core.Bases;assembly=Nakara.Framework"
    xmlns:controls="clr-namespace:Nakara.Controls;assembly=Nakara.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Nakara.Modules.PersonalInformation.UI.SeasonData.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:Background="Gray"
    d:DesignHeight="720"
    d:DesignWidth="1080"
    mc:Ignorable="d">
    <Grid Margin="30">
        <Grid.RowDefinitions>
            <RowDefinition Height="0.15*" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.45*" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Grid
                Grid.Column="0"
                Margin="0,0,10,0"
                Background="#3A000000" />
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="0.2*" />
                    <RowDefinition />
                    <RowDefinition Height="0.3*" />
                </Grid.RowDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Margin="10,10,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    FontSize="30"
                    Foreground="White"
                    Text="当前赛季" />
                <TextBlock
                    Grid.Row="2"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Top"
                    FontSize="30"
                    Foreground="White"
                    Text="蚀月2" />
            </Grid>
            <Grid Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="0.5*" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <UniformGrid
                    Margin="0,0,0,10"
                    Background="#3A000000"
                    Columns="6">
                    <StackPanel
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Orientation="Vertical">
                        <controls:IconTextBlockCustomControl Text="游戏场次" TextFontSize="15" />
                        <TextBlock
                            HorizontalAlignment="Center"
                            FontSize="15"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="31" />
                    </StackPanel>
                    <StackPanel
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Orientation="Vertical">
                        <controls:IconTextBlockCustomControl Text="天选次数" TextFontSize="15" />
                        <TextBlock
                            HorizontalAlignment="Center"
                            FontSize="15"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="31" />
                    </StackPanel>
                    <StackPanel
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Orientation="Vertical">
                        <controls:IconTextBlockCustomControl Text="前五数" TextFontSize="15" />
                        <TextBlock
                            HorizontalAlignment="Center"
                            FontSize="15"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="31" />
                    </StackPanel>
                    <StackPanel
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Orientation="Vertical">
                        <controls:IconTextBlockCustomControl Text="击败" TextFontSize="15" />
                        <TextBlock
                            HorizontalAlignment="Center"
                            FontSize="15"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="31" />
                    </StackPanel>
                    <StackPanel
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Orientation="Vertical">
                        <controls:IconTextBlockCustomControl Text="淘汰" TextFontSize="15" />
                        <TextBlock
                            HorizontalAlignment="Center"
                            FontSize="15"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="31" />
                    </StackPanel>
                    <StackPanel
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Orientation="Vertical">
                        <controls:IconTextBlockCustomControl Text="击败/被击败" TextFontSize="15" />
                        <TextBlock
                            HorizontalAlignment="Center"
                            FontSize="15"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="31" />
                    </StackPanel>
                </UniformGrid>
                <UniformGrid Grid.Row="1" Columns="3">
                    <UniformGrid
                        Margin="0,0,5,0"
                        Background="#3A000000"
                        Rows="3">
                        <UniformGrid Columns="2">
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="最长生存时间" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="24:22" />
                            </StackPanel>
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="场均生存时间" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="9:16" />
                            </StackPanel>
                        </UniformGrid>
                        <UniformGrid Columns="2">
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="最长移动距离" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="24:22" />
                            </StackPanel>
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="场均移动距离" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="9:16" />
                            </StackPanel>
                        </UniformGrid>
                        <UniformGrid Columns="2">
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="天选率" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="24:22" />
                            </StackPanel>
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="前五率" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="9:16" />
                            </StackPanel>
                        </UniformGrid>
                    </UniformGrid>
                    <UniformGrid
                        Margin="5,0"
                        Background="#3A000000"
                        Rows="3">
                        <UniformGrid Columns="2">
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="单局最高击杀" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="24:22" />
                            </StackPanel>
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="场均击杀" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="9:16" />
                            </StackPanel>
                        </UniformGrid>
                        <UniformGrid Columns="2">
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="单局最高伤害" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="24:22" />
                            </StackPanel>
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="场均伤害" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="9:16" />
                            </StackPanel>
                        </UniformGrid>
                        <UniformGrid Columns="2">
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="单局最高助攻" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="24:22" />
                            </StackPanel>
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="场均助攻" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="9:16" />
                            </StackPanel>
                        </UniformGrid>
                    </UniformGrid>
                    <UniformGrid
                        Margin="5,0,0,0"
                        Background="#3A000000"
                        Rows="3">
                        <UniformGrid Columns="2">
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="场均治疗量" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="24:22" />
                            </StackPanel>
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="总救援队有数" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="9:16" />
                            </StackPanel>
                        </UniformGrid>
                        <UniformGrid Columns="2">
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="降龙次数" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="24:22" />
                            </StackPanel>
                            <StackPanel
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Orientation="Vertical">
                                <TextBlock Foreground="White" Text="箭破天穹次数" />
                                <TextBlock
                                    HorizontalAlignment="Center"
                                    Foreground="White"
                                    Text="9:16" />
                            </StackPanel>
                        </UniformGrid>
                    </UniformGrid>
                </UniformGrid>
            </Grid>
        </Grid>
    </Grid>
</bases:UserControlBase>